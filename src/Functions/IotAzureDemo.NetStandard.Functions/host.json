{
  "logger": {
    "categoryFilter": {
      "defaultLevel": "Information",
      "categoryLevels": {
        "Host.Results": "Information",
        "Host.Executor": "Warning",
        "Host.General": "Warning",
        "Host.Controllers.Admin": "Warning",
        "Function": "Warning",
        "Host.Aggregator": "Information"
      }
    },
    "aggregator": {
      "batchSize": 1000,
      "flushTimeout": "00:00:30"
    }
  },
  "applicationInsights": {
    "sampling": {
      "isEnabled": true,
      "maxTelemetryItemsPerSecond": 5
    }
  },
  "http": {
    "routePrefix": ""
  }
}